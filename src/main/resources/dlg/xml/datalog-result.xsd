<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Georg (Nitsche) --><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:element name="datalog-result">		<xs:annotation>			<xs:documentation>This schema is designed for documents, which contain evaluation results of a Datalog query</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="analysis">					<xs:complexType>						<xs:sequence>							<xs:element name="summary" minOccurs="0"/>							<xs:element name="grading" minOccurs="0"/>							<xs:element name="syntax" minOccurs="0"/>							<xs:element name="consistency" minOccurs="0"/>							<xs:element name="error" minOccurs="0" maxOccurs="unbounded">								<xs:complexType>									<xs:simpleContent>										<xs:extension base="xs:string">											<xs:attribute name="code" use="optional">												<xs:simpleType>													<xs:restriction base="xs:ID">														<xs:enumeration value="missing-predicate"/>														<xs:enumeration value="redundant-predicate"/>														<xs:enumeration value="low-arity-predicate"/>														<xs:enumeration value="high-arity-predicate"/>														<xs:enumeration value="missing-fact"/>														<xs:enumeration value="redundant-fact"/>														<xs:enumeration value="negative-fact"/>														<xs:enumeration value="positive-fact"/>													</xs:restriction>												</xs:simpleType>											</xs:attribute>											<xs:attribute name="title" type="xs:string" use="optional"/>										</xs:extension>									</xs:simpleContent>								</xs:complexType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="query">					<xs:complexType>						<xs:simpleContent>							<xs:extension base="xs:string"/>						</xs:simpleContent>					</xs:complexType>				</xs:element>				<xs:element name="model" minOccurs="0" maxOccurs="unbounded">					<xs:complexType>						<xs:sequence>							<xs:element name="predicate" minOccurs="0" maxOccurs="unbounded">								<xs:complexType>									<xs:sequence>										<xs:element name="fact" minOccurs="0" maxOccurs="unbounded">											<xs:complexType>												<xs:sequence>													<xs:element name="term" minOccurs="0" maxOccurs="unbounded">														<xs:complexType>															<xs:attribute name="value" use="required"/>														</xs:complexType>													</xs:element>												</xs:sequence>												<xs:attribute name="error" use="optional">													<xs:simpleType>														<xs:restriction base="xs:IDREF">															<xs:enumeration value="missing-predicate"/>															<xs:enumeration value="redundant-predicate"/>															<xs:enumeration value="low-arity-predicate"/>															<xs:enumeration value="high-arity-predicate"/>															<xs:enumeration value="missing-fact"/>															<xs:enumeration value="redundant-fact"/>															<xs:enumeration value="negative-fact"/>															<xs:enumeration value="positive-fact"/>														</xs:restriction>													</xs:simpleType>												</xs:attribute>												<xs:attribute name="negated" type="xs:boolean" use="required"/>											</xs:complexType>										</xs:element>									</xs:sequence>									<xs:attribute name="name" use="required"/>									<xs:attribute name="arity" type="xs:nonNegativeInteger" use="required"/>									<xs:attribute name="error" use="optional">										<xs:simpleType>											<xs:restriction base="xs:IDREF">												<xs:enumeration value="missing-predicate"/>												<xs:enumeration value="redundant-predicate"/>												<xs:enumeration value="low-arity-predicate"/>												<xs:enumeration value="high-arity-predicate"/>												<xs:enumeration value="missing-fact"/>												<xs:enumeration value="redundant-fact"/>												<xs:enumeration value="negative-fact"/>												<xs:enumeration value="positive-fact"/>											</xs:restriction>										</xs:simpleType>									</xs:attribute>									<xs:attribute name="required" type="xs:boolean" use="optional"/>									<xs:attribute name="filter" type="xs:boolean" use="optional"/>								</xs:complexType>							</xs:element>						</xs:sequence>						<xs:attribute name="consistent" type="xs:boolean" use="required"/>					</xs:complexType>				</xs:element>			</xs:sequence>			<xs:attribute name="exercise-id" type="xs:string" use="optional"/>			<xs:attribute name="date" type="xs:date" use="required"/>			<xs:attribute name="max-score" type="xs:decimal" use="optional"/>			<xs:attribute name="score" type="xs:decimal" use="optional"/>			<xs:attribute name="max-int" type="xs:positiveInteger" use="required"/>			<xs:attribute name="diagnose-level" type="xs:int" use="optional"/>			<xs:attribute name="syntax-error" type="xs:boolean" use="required"/>			<xs:attribute name="timeout-error" type="xs:boolean" use="required"/>			<xs:attribute name="analyzable" type="xs:boolean" use="required"/>		</xs:complexType>	</xs:element></xs:schema>