<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">

  </persistence-unit>
    <persistence-unit name="FD">
      <class>at.jku.dke.etutor.modules.fd.entities.FunctionalDependency</class>
        <class>at.jku.dke.etutor.modules.fd.entities.Task</class>
        <class>at.jku.dke.etutor.modules.fd.entities.Relation</class>
        <class>at.jku.dke.etutor.modules.fd.entities.Solution</class>
        <class>at.jku.dke.etutor.modules.fd.entities.Closure</class>
        <class>at.jku.dke.etutor.modules.fd.entities.MinimalCover</class>
        <class>at.jku.dke.etutor.modules.fd.entities.Key</class>
        <properties>
        <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/postgres"/>
        <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
        <property name="hibernate.connection.username" value="etutor"/>
        <property name="hibernate.connection.password" value="etutor"/>
      </properties>
    </persistence-unit>
</persistence>
